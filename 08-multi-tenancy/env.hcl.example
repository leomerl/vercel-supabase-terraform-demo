# Environment variables for multi-tenancy deployment
# Copy this file to env.hcl and fill in your actual values

locals {
  # Supabase Configuration
  supabase_access_token    = "your-supabase-access-token"
  supabase_organization_id = "your-supabase-org-id"

  # Vercel Configuration
  vercel_api_token = "your-vercel-api-token"

  # Default region for all tenants (can be overridden per tenant)
  region = "us-east-1"

  # Default framework for all tenants (can be overridden per tenant)
  framework = "nextjs"
}